<UserControl x:Class="JeanRichard.BatLogger.Wpf.Controls.BatLocationPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModels="clr-namespace:JeanRichard.BatLogger.Wpf.ViewModels"
             xmlns:controls="clr-namespace:JeanRichard.BatLogger.Wpf.Controls"
             d:DataContext="{d:DesignInstance controls:TimeLineBar}"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <Label Content="Sensor" />
        <Label Content="{Binding Path=TotalCallCount}" />
        <Label Content="{Binding Path=MeanFrequency}" />
    </StackPanel>
</UserControl>
